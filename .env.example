
# Replace these with your actual Stripe API keys from your Stripe dashboard.
# You can find these at https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# Replace these with your actual Stripe Price IDs.
# You can create products and prices in your Stripe dashboard.
NEXT_PUBLIC_DEVELOPER_PRICE_ID=
NEXT_PUBLIC_DEVELOPER_YEARLY_PRICE_ID=
NEXT_PUBLIC_TEAM_PRICE_ID=
NEXT_PUBLIC_TEAM_YEARLY_PRICE_ID=

# This should be the public URL of your application.
# For local development, it's typically http://localhost:3000
# In Firebase Studio, this will be the preview URL.
NEXT_PUBLIC_URL=http://localhost:3000

# Replace these with your GitHub App credentials
# You can create a GitHub App at https://github.com/settings/apps
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GITHUB_APP_ID=
GITHUB_WEBHOOK_SECRET=
NEXT_PUBLIC_GITHUB_APP_NAME=
GITHUB_PRIVATE_KEY=

# These are your Firebase project credentials.
# They are typically configured when you set up the project.
NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=
FIREBASE_PROJECT_ID=
FIREBASE_CLIENT_EMAIL=
FIREBASE_PRIVATE_KEY=
